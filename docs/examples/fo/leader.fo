      -->
      <fo:list-block provisional-distance-between-starts="0.3cm"
                     provisional-label-separation="0.15cm">
        
        <!-- list item -->
        <fo:list-item>
          <!-- insert a bullet -->
          <fo:list-item-label>
            <fo:block><fo:inline font-size="10pt" font-family="Symbol">&#183;</fo:inline></fo:block>
