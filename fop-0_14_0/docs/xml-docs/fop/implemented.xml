  <s2 title="3) SVG elements supported">
    <ul>
      <li>rect</li>
      <li>circle</li>
      <li>line</li>
      <li>text - tref, tspan</li>
      <li>path</li>
      <li>polygon</li>
      <li>ellipse</li>
      <li>g</li>
      <li>svg (inside)</li>
      <li>image</li>
      <li>use</li>
      <li>defs (including referenced items not in defs element)</li>
      <li>linearGradient</li>
      <li>radialGradient</li>
      <li>stop</li>
    </ul>
  </s2>
