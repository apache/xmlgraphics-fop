		<keyword-equiv match="normal" eval="true">1.2em</keyword-equiv>
		<datatype-conversion from-type="Number" varname="numval">
		new PercentLength(numval.doubleValue(),
				 getPercentBase(fo,propertyList))</datatype-conversion>
